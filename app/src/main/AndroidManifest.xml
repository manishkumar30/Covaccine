<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covaccine">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logocovaccine"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logocovaccine"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covaccine">
        <activity android:name=".Statelist"></activity>
        <activity android:name=".covidTracker" />
        <activity android:name=".Slotdetails" />
        <activity android:name=".Slotdisplay" />
        <activity android:name=".Search" />
        <activity android:name=".MainPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>